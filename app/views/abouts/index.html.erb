<header>
        <div class="row">
            <h1>Latest Uploads The Last 24 hrs.</h1>
                <% @practicals.each do |practical|%>
                    <article class="four columns featured_post">
                        <h3><%=link_to practical.aim,practical%></h3> 
                        <% %>
                        <%=link_to image_tag(practical.images.first.sheet.url,alt:"desc",style:"width:100%;height:200px;"),practical %>
                        <strong><%=simple_format(practical.description)%></strong>

                        <div class="post_meta">
                            <i class="fa fa-clock-o" aria-hidden="true"> <%=time_ago_in_words(practical.created_at)%></i>
                            <i class="fa fa-user" aria-hidden="true" style="margin-left:15px"> Admin</i>
                            <i class="fa fa-comments" aria-hidden="true" style="margin-left: 15px">5</i>
                        </div>
                    </article>
                <% end %>
        </div>
        <div class="row">
                <% @examnotes.each do |examnote|%>
                    <article class="four columns featured_post">
                        <h3><%=link_to examnote.topic,examnote%></h3> 
                        <% %>
                       <%=link_to image_tag(examnote.notes.first.page.url,alt:"desc",style:"width:100%;height:200px;"),examnote %>
                        <p><%=simple_format(examnote.tips)%></p>

                        <div class="post_meta">
                            <i class="fa fa-clock-o" aria-hidden="true"> <%=time_ago_in_words(examnote.created_at)%></i>
                            <i class="fa fa-user" aria-hidden="true" style="margin-left:15px"> Admin</i>
                            <i class="fa fa-comments" aria-hidden="true" style="margin-left: 15px">5</i>
                        </div>
                    </article>
                <% end %>
        </div>
        <div class="row">
                <% @assignments.each do |assignment|%>
                    <article class="four columns featured_post">
                        <h3><%=link_to assignment.topic,assignment%></h3> 
                        <% %>
                        <%=link_to image_tag(assignment.aimages.first.image.url,alt:"desc",style:"width:100%;height:200px;"),assignment%>
                        <p><%=simple_format(assignment.description)%></p>

                        <div class="post_meta">
                            <i class="fa fa-clock-o" aria-hidden="true"> <%=time_ago_in_words(assignment.created_at)%></i>
                            <i class="fa fa-user" aria-hidden="true" style="margin-left:15px"> Admin</i>
                            <i class="fa fa-comments" aria-hidden="true" style="margin-left: 15px">5</i>
                        </div>
                    </article>
                <% end %>
        </div>

</header>
