<header>
    <h1 class="welcome_text">Edit Practical</h1>    
</header>
<section class="section_light">
    <div class="row" style="padding-top: 20px">
      	<div class="large-6 large-offset-3 columns">
      		<% if @practical.errors.any?%>
				<div id="errors">
					<h2><%= pluralize(@practical.errors.count, "Error")%> prevented this practical from saving</h2>
					<ul>
						<% @practical.errors.full_messages.each do |msg| %>
							<li><%= msg%></li>
						<% end%>	
					</ul>
				</div>
			<% end %>
			<%=render('form')%>
			<br>
			<%=link_to "Back",:back,class:"button"%>
      	</div>
    </div>
</section>


















