<header>
    <h1 class="welcome_text"><%= @practical.aim %></h1>    
</header>
<section class="section_light">
	<div class="row">
		<div id="container">
			<% @practical.images.each do |image| %>
				<div class='box photo col2'>
					<a href="#"><%= image_tag(image.sheet.url,alt:"desc")%></a>
				</div>
			<%end%>
		</div>
	</div>
		<div id="container">
	<h4>Practical Info</h4>

    <div class="row">
      <div class="twelve columns">

      <dl class="tabs three-up">
        <dd class="active"><a href="#simple1">Aim</a></dd>
        <dd><a href="#simple2">Description</a></dd>
      </dl>
      <ul class="tabs-content">
        <li class="active" id="simple1Tab"><%= @practical.aim %></li>
       	<li id="simple2Tab"><%=simple_format(@practical.description)%></li>
      </ul>

      </div>
	</div>
	<%= link_to "Edit",edit_practical_path(@practical),class:"button"%>
	<%= link_to "Delete",@practical,method: :delete,data:{confirm:"Are you sure?"},class:"button"%>
	<%= link_to "Back",:back,class:"button"%>
	<br>
	<br>
		<div class="panel">
			<div class="row">
				<div class="six colums">
					
				</div>
			</div>
		</div>
</section>
